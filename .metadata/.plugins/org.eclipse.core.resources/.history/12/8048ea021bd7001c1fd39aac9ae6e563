package com.capgemini.basic;

public class MaximunOfThreeNumbers {

	public static void main(String[] args) {
		int n1=19;
		int n2=54;
		int n3=34;
		if(n1>n2) {
			System.out.println("n1 is greater");
		}
		else if(n2>n3) {
			System.out.println("n2 is greater");
		}
		else {
			System.out.println("n3 is greater");
		}

	}

}
