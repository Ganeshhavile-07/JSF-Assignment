package com.capgemini.assignment1;

public class ArmstrongNumber {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
        int num=153;
        int res=0;
        int rem;
        int temp=num;
        while(temp!=0) {
        	res=res*10+temp/10;
        	temp=temp/10;
        }
        System.out.println(temp);
        System.out.println(res);
	}

}
